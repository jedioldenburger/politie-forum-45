{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "dynamicImport": true
    },
    "target": "es2022",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": true,
    "transform": {
      "react": {
        "runtime": "automatic",
        "importSource": "react"
      },
      "legacyDecorator": false,
      "decoratorMetadata": false
    }
  },
  "module": {
    "type": "es6"
  },
  "env": {
    "targets": {
      "chrome": "100",
      "edge": "100",
      "firefox": "100",
      "safari": "15"
    },
    "mode": "entry",
    "coreJs": false
  }
}
